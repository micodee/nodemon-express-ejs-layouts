<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>About</title>
  </head>
  <body>
    <% if (namaMakanan.length<1) { %>
     <p>Data makanan masih kosong</p>
    <% } %>
    <% namaMakanan.forEach(food => { %>
    <ul>
      <li>Makanan : <%= food.makan %></li>
      <li>Harga : <%= food.harga %></li>
    </ul>
    <% }) %>
  </body>
</html>
